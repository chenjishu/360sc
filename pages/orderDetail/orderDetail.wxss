.order-detail{
background: #fff;
}
.order-detail>.order-title{
padding:10px 20px;font-size:16px;background: #eee;color:#666;font-weight:bold;
}
.order-detail>.order-list{
  padding:0 20px;
}
.order-detail>.order-list>view{
  border-bottom:1px solid #eee;display:flex;font-size:12px;padding:12px 0;line-height:20px;
}
.order-detail>.order-list>view>.left{
  width:80px;color:#999;
  
}
.order-detail>.order-list>view>.right{
  flex:1
}
.bot{
  position:fixed;bottom:0;left:0;right:0;height:50px;border-top:1px solid #eee;background: #F7F9FC;
}
.bot>view{
  float:right;width:80px;margin-right:10px;height:24px;line-height:24px;border:1px solid #999;text-align:center;font-size:14px;color:#999;border-radius: 15px;margin-top:13px;
}