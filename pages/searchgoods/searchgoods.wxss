page{
  background:#eee;
}
.search-wrapper{
  padding:10px;background: #333;display:flex;flex-direction: row;
}
.search-wrapper>.input-wrapper{
 width:80%;height:30px;background-color: #fff;border-radius: 15px;
  background-image:url('../../images/search.png')no-repeat;
  background-size:30px;
}
.search-wrapper>.input-wrapper>input{
  font-size:16px;color:#999;
 height:30px;width:100%;padding-left:15px;box-sizing:border-box;background: none;background-image:url('/images/search.png')no-repeat;
}
.search-wrapper .img-wrapper{
  font-size:16px;color:#fff;line-height:30px;display:block;background: none;border-color:#ADCBE1;height:30px;border:none;
}
.search-wrapper .img-wrapper>image{
  width:20px;height:20px;
}
.nav{
  display:flex;padding:10px;
}
.nav>.box{
  flex:1;text-align:center;font-size:14px;height:40px;line-height:40px;border-radius: 5px;margin-top:10px;
}
.nav>.box.active{
  background:#ADCBE1;color:#fff;
}
.goods:after{
  display:'table';
  content:'';
  clear:both;
}
.goods{
 border-bottom:1px solid #eee;
}
.goods>.good-wrapper{
  float:left;width:50%;box-sizing: border-box;background:#fff;
}
.goods>.good-wrapper:first-child,.goods>.good-wrapper:nth-child(2){
 border-top:1px solid #eee;
}
.goods>.good-wrapper:nth-child(2n+1){
border-right:1px solid #eee;
}
.goods>.good-wrapper:nth-child(2n){
border-left:0;
}
.goods .good{
  border:1px solid #eee;text-align:center;border-top:none;padding-bottom:10px;

}
.goods .good:nth-child(2n+1){
border-right:0;
}
.goods .good:nth-child(2n){
  border-left:0;
}
.goods .good>image{
  width:200rpx;height:200rpx;
}
.goods .good>.good-name{
  width:90%;margin:0 auto;height:20px;font-size:14px;color:#333;overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;line-height:20px;
text-align:left;
}
.goods .good>.good-in{
  width:90%;margin:0 auto;height:20px;font-size:12px;color:#999;overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;line-height:20px;text-align:left;
}
.do{
  display:flex;align-items:center;font-size:16px;color:red;width:90%;margin:6px auto 0;
}
.do>view{
  flex:1;text-align:left;padding-bottom: 4px;
}
.do>.like{
  text-align:right;
}
.do>view>image{
  width:20px;height:20px;margin-right:10px;vertical-align: middle
}